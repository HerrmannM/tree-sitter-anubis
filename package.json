  "gypfile": true,
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HerrmannM/tree-sitter-anubis.git"
  },
  "bugs": {
    "url": "https://github.com/HerrmannM/tree-sitter-anubis/issues"
